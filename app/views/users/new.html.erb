<h1>Sign Up</h1>



  <form action="<%= users_url %>" method="post">
    <%= auth_token %>

    <label>Username
      <input type="text" name="user[username]" id="Username" value="">
    </label>

    <label>Email
      <input type="email" name="user[email]" id="Username" value="">
    </label>

    <label>Password
      <input type="password" name="user[password]" id="Password" value="">
    </label>

    <label>Confirm Password
      <input type="password" name="user[password_confirmation]" id="Confirm_Password" value="">
    </label>

    <label>Location
      <input type="text" name="user[location]" id="Location" value="">
    </label>

    <label>Birthday
      <input type="date" name="user[birthday]" id="Birthday" value="">
    </label>


    <input type="submit" value="Sign Up">
  </form>
